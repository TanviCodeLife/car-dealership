@using CarDealership.Models;

<h1>Car Dealership</h1>
<ul>

  @if (Model.Count == 0)
  {
    <p>You have no cars in your list</p>
  }
  @if (Model.Count != 0)
  {
    @foreach (Car car in Model)
    {
      <li>@car.GetMakeModel()</li>
      <li>@car.GetPrice()</li>
      <li>@car.GetMiles()</li>
      <li>@car.GetNote()</li>
      <p>--------------</p>
      <br>
    }
  }

    </ul>
    <a href="/cars/new">Add a new item.</a>
